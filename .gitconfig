[core]
  excludesfile = ~/.gitignore
	editor = nvim
[push]
  default = upstream
[color]
  ui = true
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[commit]
	gpgsign = false
[pull]
	rebase = true
